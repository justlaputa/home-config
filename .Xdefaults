!!==== urxvt configurations ====!!
URxvt.urgentOnBell: True
URxvt.transparent: false
!URxvt*tintColor: Blue
URxvt.shading: 40
URxvt.saveLines: 12000
URxvt.font: xft:Inconsolata:pixelsize=16:antialias=true:hinting=true, WenQuanYi Micro Hei Mono:pixelsize=16:antialias=true:hinting=true
URxvt.boldFont: xft:Inconsolata:pixelsize=16:antialias=true:hinting=true, WenQuanYi Micro Hei Mono:pixelsize=16:antialias=true:hinting=true
URxvt.scrollBar: false
URxvt.geometry: 100x40+0+25
URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:      /usr/bin/firefox
URxvt.matcher.button:   1 
URxvt.scrollTtyOutput: False
URxvt.scrollTtyKeypress: True
URxvt.scrollWithBuffer: True
URxvt.preeditType:Root
!cursor
URxvt.underlineColor: green
URxvt.cursorUnderline: true
URxvt.cursorBlink: true
URxvt.cursorColor:  #dc74d1
!color theme
URxvt.foreground:     grey80
URxvt.background:     #080808
!!black
URxvt.color0:         grey0
URxvt.color8:         grey10
!!red
URxvt.color1:         red3
URxvt.color9:         red1
!!green
URxvt.color2:         LawnGreen
URxvt.color10:        green1
!!yellow (orange)
URxvt.color3:         yellow1
URxvt.color11:        orange1
!!blue
URxvt.color4:         DodgerBlue3
URxvt.color12:        DodgerBlue1
!!magenta
URxvt.color5:         magenta2
URxvt.color13:        magenta1
!!!cyan
URxvt.color6:         cyan3
URxvt.color14:        cyan1
!!!white
URxvt.color7:         FloralWhite
URxvt.color15:        GhostWhite
